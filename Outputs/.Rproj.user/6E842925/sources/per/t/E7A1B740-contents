
setwd("P:/LANDIS II/R_calculus")

#install.packages("raster")
library(raster)

#install.packages("rgdal")
#library(rgdal)

#install.packages("rgeos")
#library(rgeos)

Ownership_TotKlamath = raster("P:/LANDIS II/Biomass Harvest - trial/E4_ownership_BAU_v4.tif")


Stands_TotKlamath = raster("P:/LANDIS II/Biomass Harvest - trial/E4_stands_BAU_v5.tif")


IC_OnlyCA = raster("P:/LANDIS II/Biomass Harvest - trial/klamath_IC.tif")

IC_OnlyCA[IC_OnlyCA == 0] <- NA

Ownership_OnlyCA = crop(x = Ownership_TotKlamath, y = IC_OnlyCA)

Stands_OnlyCA = intersect(x=Stands_TotKlamath, y = IC_OnlyCA)

par(mfrow=c(3,2))

plot(Stands_TotKlamath)
plot(Ownership_TotKlamath)

plot(Stands_OnlyCA)
plot(Ownership_OnlyCA)

plot(IC_OnlyCA)
